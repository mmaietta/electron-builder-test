{
  "name": "electron-quick-start-typescript",
  "version": "1.0.2",
  "description": "A minimal Electron application written with Typescript",
  "main": "out/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "pnpm build && electron ./out/main.js",
    "app:pack": "pnpm electron-builder --publish never",
    "app:dist": "pnpm electron-builder --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": {
    "name": "GitHub",
    "email": "test@test.com"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron/fuses": "^1.6.1",
    "@electron/rebuild": "^3.2.12",
    "@types/node": "^18.7.4",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "electron": "^23",
    "electron-builder": "^24.1.3",
    "eslint": "^7.32.0",
    "lazy-val": "^1.0.5",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "electron-log": "^4.4.1",
    "electron-updater": "^6",
    "node-mac-permissions": "^2.3.0"
  }
}
