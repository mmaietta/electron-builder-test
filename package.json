{
  "name": "electron-quick-start-typescript",
  "version": "1.0.2",
  "description": "A minimal Electron application written with Typescript",
  "main": "out/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "pnpm build && electron ./out/main.js",
    "app:pack": "electron-builder --config electron-builder-config.js --publish never",
    "app:dist": "electron-builder --config electron-builder-config.js --publish always",
    "postinstall": "patch-package && electron-builder install-app-deps"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": {
    "name": "GitHub",
    "email": "test@test.com"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "^18.7.4",
    "@types/semver": "^7.5.7",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "basic-ftp": "^5.0.4",
    "electron": "^13.1.7",
    "electron-builder": "^24.13.2",
    "eslint": "^7.32.0",
    "lazy-val": "^1.0.5",
    "patch-package": "^8.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "dotenv": "^16.4.4",
    "electron-log": "^4.4.1",
    "electron-updater": "^6"
  }
}
